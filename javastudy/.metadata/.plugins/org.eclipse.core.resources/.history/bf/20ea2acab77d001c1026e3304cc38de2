package ex03_arithmetic;

public class Ex02_operator {

	public static void main(String[] args) {
		
		// 1씩 증가/감소하기
		
		int a = 10;
		int b = 10;
		
		int res1 = a++;
		int res2 = ++b;
		
		System.out.println(a);
		System.out.println(res1);
		
		System.out.println(b);
		System.out.println(res2);
		
		

	}

}
