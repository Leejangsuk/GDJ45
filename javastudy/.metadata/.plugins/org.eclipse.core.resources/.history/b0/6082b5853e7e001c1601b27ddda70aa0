package ex03_arithmetic;

public class Ex04_operator {

	public static void main(String[] args) {
		
		// 복합 대입 연산
		// +=, -=, *=, /=, %=
		
		int wallet = 0;
			
		wallet = wallet + 30000;  // 기존 wallet의 값을 30000 늘린다.
		
		wallet += 30000;          // 동일한 코드.
		
		System.out.println(wallet);

	}

}
